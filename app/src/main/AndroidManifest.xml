<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="rbdavis.familymap">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.screens.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.screens.PersonActivity" />
        <activity
            android:name=".ui.screens.SettingsActivity"
            android:label="@string/title_activity_settings" />
        <activity android:name=".ui.screens.EventActivity" />
        <activity android:name=".ui.screens.SearchActivity" />
        <activity android:name=".ui.screens.FilterActivity"></activity>
    </application>

</manifest>